DB_HOST=localhost
DB_USER=root
DB_PASSWORD=password
DB_NAME=myportal
# Allow multiple SQL statements in a single query (disabled by default)
# DB_ALLOW_MULTIPLE_STATEMENTS=false
# SESSION_SECRET must be a high-entropy value (e.g. generated with `openssl rand -hex 32`)
SESSION_SECRET=
# 32-byte key used to encrypt TOTP secrets
TOTP_ENCRYPTION_KEY=
# Secret used to HMAC-hash API keys
API_KEY_SECRET=
XERO_WEBHOOK_URL=
XERO_API_KEY=
SYNCRO_WEBHOOK_URL=
SYNCRO_API_KEY=
LICENSES_WEBHOOK_URL=
LICENSES_WEBHOOK_API_KEY=
SHOP_WEBHOOK_URL=
SHOP_WEBHOOK_API_KEY=
VERIFY_WEBHOOK_URL=
VERIFY_API_KEY=
SMS_ENDPOINT=http://172.16.255.2:8080/message
SMS_AUTH=
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
SMTP_SECURE=
SMTP_FROM=
STOCK_FEED_URL=
CRON_TIMEZONE=Etc/UTC
# Base URL (path or absolute) that points to the OpnForm instance proxied by nginx
# Defaults to /myforms/ when unset
# OPNFORM_BASE_URL=/myforms/
# Optional file target for authentication logs consumed by fail2ban
# FAIL2BAN_LOG_PATH=/var/log/myportal/auth.log
